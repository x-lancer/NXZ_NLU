{
  "model_name": "paraphrase-multilingual-MiniLM-L12-v2",
  "model_type": "minilm",
  "max_length": 128,
  "batch_size": 1,
  "device": "cpu",
  "use_gpu": false,
  "similarity_threshold": 0.6,
  "intent_labels": [
    "vehicle_control",
    "vehicle_query",
    "unknown"
  ],
  "preprocessing": {
    "lowercase": false,
    "remove_punctuation": false,
    "normalize_embeddings": true
  },
  "description": "使用 MiniLM 轻量级模型进行意图识别，通过文本嵌入和相似度匹配实现"
}

